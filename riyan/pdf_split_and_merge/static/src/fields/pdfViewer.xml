<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="pdf_split_and_merge.PdfViewerDialog">
        <Dialog title="'New Project'" size="'lg'" header="false" footer="false">
            <div class="d-flex w-100 align-items-center justify-content-end">
                <a href="#" t-on-click="props.close">
                    <i class="fa fa-close" style="font-size: 20px; color: #000000;"></i>
                </a>
            </div>
            <iframe class="w-100" style="min-height: 80vh;"
                alt="PDF file"
                t-att-src="props.url"
                t-att-name="props.name"
                t-on-error="onLoadFailed"
            />
        </Dialog>
    </t>
</templates>

